    "cross-env": "^7.0.3",
    "custom-event-polyfill": "^1.0.7",
    "eslint": "^8.27.0",
    "font-awesome": "^4.7.0",
    "jest": "^29.3.1",
    "jest-extended": "^3.1.0",
    "jest-preset-angular": "^12.2.2",
    "moment-mini": "^2.29.4",
    "ng-packagr": "^14.2.2",
    "ngx-bootstrap": "^9.0.0",
    "npm-run-all2": "^6.0.4",
    "postcss": "^8.4.19",
    "postcss-cli": "^10.0.0",
    "release-it": "^15.5.0",
    "rimraf": "^3.0.2",
    "sass": "^1.56.1",
    "stream-browserify": "^3.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "~4.7.4",
    "zone.js": "~0.12.0"
  },
  "engines": {
    "node": ">=14.17.0",
    "npm": ">=6.14.13"
  },
  "resolutions": {
    "caniuse-lite": "1.0.30001431"
  }
}
