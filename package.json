    "eslint-plugin-n": "^17.17.0",
    "jsdom": "^26.1.0",
    "native-copyfiles": "^0.3.2",
    "ng-packagr": "^18.2.1",
    "ngx-bootstrap": "^18.1.3",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "release-it": "^19.0.1",
    "rimraf": "^5.0.10",
    "rxjs": "^7.8.2",
    "sass": "^1.87.0",
    "servor": "^4.0.2",
    "sortablejs": "^1.15.6",
    "stream-browserify": "^3.0.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.2",
    "zone.js": "~0.15.0"
  },
  "engines": {
    "node": ">=18.19.1"
  },
  "resolutions": {
    "caniuse-lite": "1.0.30001715",
    "express": "^4.21.2",
    "semver": "^7.7.1",
    "string-width": "4.2.3",
    "wrap-ansi": "7.0.0",
    "ws": "^8.18.1"
  }
}
